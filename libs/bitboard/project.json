{
    "name": "bitboard",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "lib/bitboard/src",
    "targets": {
        "build": {
            "executor": "@monodon/rust:build",
            "outputs": ["{options.target-dir}"],
            "options": {
                "target-dir": "dist/libs/bitboard"
            },
            "configurations": {
                "production": {
                    "release": true
                }
            }
        },
        "test": {
            "executor": "@monodon/rust:test",
            "outputs": ["{options.target-dir}"],
            "options": {
                "target-dir": "dist/libs/bitboard",
                "lib": true
            },
            "configurations": {
                "production": {
                    "release": true
                }
            }
        },
        "lint": {
            "executor": "@monodon/rust:lint",
            "outputs": ["{options.target-dir}"],
            "options": {
                "target-dir": "dist/libs/bitboard"
            }
        }
    },
    "tags": []
}
