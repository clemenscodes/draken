{
    "name": "draken",
    "version": "0.0.0",
    "license": "MIT",
    "private": true,
    "scripts": {
        "all": "pnpm nx run-many --output-style=stream --skip-nx-cache -t",
        "affected": "pnpm nx affected --output-style=stream -t",
        "test:all": "pnpm all test",
        "test:affected": "pnpm affected test",
        "build:all": "pnpm all build",
        "build:affected": "pnpm affected build",
        "lint:all": "pnpm all lint",
        "lint:affected": "pnpm affected lint",
        "fmt:check": "pnpm nx format:check --all && cargo fmt --all --check",
        "fmt": "cargo fmt --all && pnpm nx format --all",
        "coverage:all": "pnpm all coverage",
        "coverage:affected": "pnpm affected coverage",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@tauri-apps/cli": "^2.0.0-beta.0",
        "internal-ip": "^7.0.0",
        "@commitlint/cli": "^18.2.0",
        "@commitlint/config-conventional": "^18.1.0",
        "@commitlint/config-nx-scopes": "^18.3.0",
        "@monodon/rust": "^1.3.2",
        "@nx/eslint": "18.0.1",
        "@nx/eslint-plugin": "18.0.1",
        "@nx/jest": "18.0.1",
        "@nx/js": "18.0.1",
        "@nx/next": "18.0.1",
        "@swc-node/register": "~1.6.7",
        "@swc/core": "~1.3.85",
        "@swc/helpers": "~0.5.2",
        "@testing-library/react": "14.0.0",
        "@types/jest": "^29.4.0",
        "@types/node": "18.16.9",
        "@types/react": "18.2.33",
        "@types/react-dom": "18.2.14",
        "@typescript-eslint/eslint-plugin": "^6.13.2",
        "@typescript-eslint/parser": "^6.13.2",
        "babel-jest": "^29.4.1",
        "commitizen": "^4.3.0",
        "commitlint": "^18.2.0",
        "cz-git": "^1.7.1",
        "eslint": "~8.48.0",
        "eslint-config-next": "14.0.4",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "husky": "^8.0.3",
        "jest": "^29.4.1",
        "jest-environment-jsdom": "^29.4.1",
        "nx": "18.0.1",
        "prettier": "^3.2.4",
        "ts-jest": "^29.1.0",
        "ts-node": "10.9.1",
        "typescript": "~5.3.2"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-git"
        }
    },
    "dependencies": {
        "@tauri-apps/api": "^2.0.0-beta.0",
        "@tauri-apps/plugin-shell": "^2.0.0-beta.0",
        "next": "14.0.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "tslib": "^2.3.0"
    }
}
